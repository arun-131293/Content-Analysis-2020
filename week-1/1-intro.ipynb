    "initial = 'https://en.wikipedia.org/wiki/Censorship'\n",
    "urlsonly = set([initial]) \n",
    "otherPAgeURLS = [(initial,0, 'censorship')]\n",
    "\n",
    "def getlinks(url,n):\n",
    "    if n==0:\n",
    "        return (urlsonly,otherPAgeURLS)\n",
    "    tempContentRequest = requests.get(url)\n",
    "    wikiContentSoup = bs4.BeautifulSoup(tempContentRequest.text, 'html.parser')\n",
    "    contentPTags = wikiContentSoup.body.findAll('p')\n",
    "    for paragraphNum, pTag in enumerate(contentPTags):\n",
    "        #we only want hrefs that link to wiki pages\n",
    "        tagLinks = pTag.findAll('a', href=re.compile('/wiki/'), class_=False)\n",
    "        for aTag in tagLinks:\n",
    "            #We need to extract the url from the <a> tag\n",
    "            relurl = aTag.get('href')\n",
    "            linkText = aTag.text\n",
    "        #wikipedia_base_url is the base we can use the urllib joining function to merge them\n",
    "        #Giving a nice structured tupe like this means we can use tuple expansion later\n",
    "            fullurl = urllib.parse.urljoin(wikipedia_base_url, relurl)\n",
    "            if fullurl not in urlsonly:\n",
    "                otherPAgeURLS.append((\n",
    "                    urllib.parse.urljoin(wikipedia_base_url, relurl),paragraphNum,linkText,))\n",
    "                urlsonly.add(urllib.parse.urljoin(wikipedia_base_url, relurl))\n",
    "                ##recursive call\n",
    "                getlinks(fullurl,n-1)\n",
    "    return (urlsonly,otherPAgeURLS)\n",
    "\n",
    "#prints out urls scraped from Censorship for two levels, recursively\n",
    "#I don't get the content from each URL, \n",
    "#as code for that is already given above and is straightforward from this point\n",
    "getlinks(initial, 2)[1]\n"
   ]
