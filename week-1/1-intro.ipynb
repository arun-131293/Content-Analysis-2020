    "wikilink = 'https://en.wikipedia.org/wiki/Censorship'\n",
    "ContentRequest = requests.get(wikilink)\n",
    "#print(ContentRequest.text[:1000])\n",
    "ContentSoup = bs4.BeautifulSoup(ContentRequest.text, 'html.parser')\n",
    "#print(ContentSoup.text[:200])\n",
    "contentPTags = ContentSoup.body.findAll('p')\n",
    "cleaned=[]\n",
    "for pTag in contentPTags:\n",
    "    #this removes any content in braces in text\n",
    "    ptext=re.sub(r'\\[[^\\[\\]]*\\]', '', pTag.text)\n",
    "    #this removes any words with numbers (usual for downstream NLP tasks)\n",
    "    ptext = re.sub(r'\\S*\\d+\\S*', '', ptext)\n",
    "    #this is a basic sentence segmenter\n",
    "    sentenceEnd = re.compile('(?<=(?<!Mr)(?<!Mrs)(?<!Ms)(?<!Dr)(?<!Jr)[.!?])\\s{1,2}(?=[A-Z])')\n",
    "    sentenceList = sentenceEnd.split(ptext)\n",
    "    for s in sentenceList:\n",
    "        temp = (s.replace('\\n',''))\n",
    "        #this removes any multiple spaces introduced by the regular expressions above\n",
    "        cleaned.append(re.sub(r' +', ' ', temp))\n",
    "#convert sentences to a DataFrame\n",
    "contentsentences = pandas.DataFrame({'wiki-sentences' : cleaned})\n",
    "print (contentsentences)\n",
    "\n"
   ]
